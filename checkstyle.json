{
    "defaultSeverity": "INFO",
    "checks": [
        {
            "type": "Anonymous"
        },
        {
            "type": "ArrayAccess",
            "props": {
                "spaceBefore": false,
                "spaceInside": false
            }
        },
        {
            "type": "ArrowFunction",
            "props": {
                "allowCurlyBody": true
            }
        },
        {
            "type": "AvoidIdentifier",
            "props": {}
        },
        {
            "type": "AvoidStarImport"
        },
        {
            "type": "BlockBreakingConditional"
        },
        {
            "type": "CatchParameterName",
            "props": {}
        },
        {
            "type": "CodeSimilarity",
            "props": {}
        },
        {
            "type": "CommentedOutCode"
        },
        {
            "type": "ConditionalCompilation",
            "props": {
                "allowSingleline": true
            }
        },
        {
            "type": "ConstantName",
            "props": {}
        },
        {
            "type": "CyclomaticComplexity",
            "props": {
                "severity": "IGNORE"
            }
        },
        {
            "type": "DefaultComesLast"
        },
        {
            "type": "DocCommentStyle",
            "props": {
                "lineStyle": "onestar",
                "startStyle": "ignore"
            }
        },
        {
            "type": "Dynamic"
        },
        {
            "type": "EmptyBlock",
            "props": {
                "option": "stmt"
            }
        },
        {
            "type": "EmptyLines",
            "props": {
                "requireEmptyLineAfterPackage": true
            }
        },
        {
            "type": "EmptyPackage",
            "props": {
                "enforceEmptyPackage": true
            }
        },
        {
            "type": "VarTypeHint",
            "props": {
                "ignoreEnumAbstractValues": false,
                "typeHintPolicy": "enforce_all"
            }
        },
        {
            "type": "ExtendedEmptyLines",
            "props": {}
        },
        {
            "type": "FileLength",
            "props": {}
        },
        {
            "type": "Final"
        },
        {
            "type": "HexadecimalLiteral",
            "props": {
                "option": "upperCase"
            }
        },
        {
            "type": "HiddenField",
            "props": {
                "ignoreConstructorParameter": true,
                "ignoreSetter": true
            }
        },
        {
            "type": "Indentation",
            "props": {
                "character": "tab"
            }
        },
        {
            "type": "IndentationCharacter",
            "props": {
                "character": "tab"
            }
        },
        {
            "type": "InnerAssignment",
            "props": {
                "ignoreReturnAssignments": false
            }
        },
        {
            "type": "LeftCurly",
            "props": {
                "option": "eol"
            }
        },
        {
            "type": "ListenerName",
            "props": {}
        },
        {
            "type": "LocalVariableName",
            "props": {}
        },
        {
            "type": "MemberName",
            "props": {}
        },
        {
            "type": "MethodCount",
            "props": {}
        },
        {
            "type": "MethodLength",
            "props": {}
        },
        {
            "type": "MethodName",
            "props": {}
        },
        {
            "type": "ModifierOrder",
            "props": {}
        },
        {
            "type": "MultipleStringLiterals",
            "props": {}
        },
        {
            "type": "MultipleVariableDeclarations",
            "props": {}
        },
        {
            "type": "NestedControlFlow",
            "props": {}
        },
        {
            "type": "NullableParameter",
            "props": {
                "option": "questionMark"
            }
        },
        {
            "type": "OperatorWhitespace",
            "props": {
                "arithmeticOpPolicy": "around",
                "arrowFunctionPolicy": "around",
                "arrowPolicy": "around",
                "assignOpPolicy": "around",
                "bitwiseOpPolicy": "around",
                "boolOpPolicy": "around",
                "compareOpPolicy": "around",
                "intervalOpPolicy": "none",
                "newFunctionTypePolicy": "around",
                "oldFunctionTypePolicy": "none",
                "ternaryOpPolicy": "around",
                "unaryOpPolicy": "none"
            }
        },
        {
            "type": "ParameterName",
            "props": {}
        },
        {
            "type": "ParameterNumber",
            "props": {}
        },
        {
            "type": "PublicAccessor"
        },
        {
            "type": "RedundantAccessMeta",
            "props": {
                "prohibitMeta": true
            }
        },
        {
            "type": "RedundantAllowMeta",
            "props": {
                "prohibitMeta": true
            }
        },
        {
            "type": "RedundantModifier",
            "props": {
                "enforcePrivate": true,
                "enforcePublic": true,
                "enforcePublicPrivate": true
            }
        },
        {
            "type": "Return",
            "props": {
                "enforceReturnType": true
            }
        },
        {
            "type": "RightCurly",
            "props": {
                "option": "same"
            }
        },
        {
            "type": "SeparatorWhitespace",
            "props": {}
        },
        {
            "type": "SeparatorWrap",
            "props": {}
        },
        {
            "type": "SimplifyBooleanExpression"
        },
        {
            "type": "SimplifyBooleanReturn"
        },
        {
            "type": "Spacing",
            "props": {}
        },
        {
            "type": "StringLiteral",
            "props": {
                "policy": "onlyDouble"
            }
        },
        {
            "type": "TabForAligning",
            "props": {}
        },
        {
            "type": "TODOComment",
            "props": {
                "format": "TODO|FIXME|XXX",
                "severity": "INFO"
            }
        },
        {
            "type": "Trace",
            "props": {
                "severity": "INFO"
            }
        },
        {
            "type": "TrailingWhitespace"
        },
        {
            "type": "Type",
            "props": {
                "ignoreEnumAbstractValues": false
            }
        },
        {
            "type": "TypeName",
            "props": {}
        },
        {
            "type": "UnnecessaryConstructor"
        },
        {
            "type": "UnusedImport",
            "props": {}
        },
        {
            "type": "UnusedLocalVar"
        },
        {
            "type": "WhitespaceAfter",
            "props": {}
        },
        {
            "type": "WhitespaceAround",
            "props": {}
        }
    ]
}
